{
  "version": 2,
  "functions": { "api/index.php": { "runtime": "vercel-php@1.0.0-rc9" } },
  "routes": [
    {
      "src": "/img/(.*)",
      "dest": "/public/assets/images/"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ],
  "outputDirectory": "public",
  "env": {
    "APP_NAME": "R.A List",
    "APP_ENV": "production",
    "APP_DEBUG": "false",
    "APP_URL": "https://ra-list.vercel.app",
    "APP_LOCALE": "en",
    "APP_FALLBACK_LOCALE": "en",
    "APP_FAKER_LOCALE": "en_US",
    "APP_CONFIG_CACHE": "/tmp/config.php",
    "APP_ROUTES_CACHE": "/tmp/routes.php",
    "VIEW_COMPILED_PATH": "/tmp",
    "CACHE_DRIVER": "array",
    "LOG_CHANNEL": "stderr",
    "SESSION_DRIVER": "cookie",
    "DB_CONNECTION": "mysql",
    "MYSQL_ATTR_SSL_CA": "/etc/pki/tls/certs/ca-bundle.crt"
  }
}
